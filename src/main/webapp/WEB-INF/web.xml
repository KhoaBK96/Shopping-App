  	<description></description>
  	<servlet-class>com.bia.web.controller.AddBillDetailController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserListController</servlet-name>
  	<url-pattern>/UserList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BillListController</servlet-name>
  	<url-pattern>/BillList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductListController</servlet-name>
  	<url-pattern>/ProductList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryListController</servlet-name>
  	<url-pattern>/CategoryList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
